{
  "name": "Extensions",
  "version": "0.12.0",
  "summary": "Swift Extensions.",
  "description": "My own Swift extensions",
  "homepage": "git@github.com:Starkrimson/Extensions.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Starkrimson": "starkrimson@icloud.com"
  },
  "source": {
    "git": "git@github.com:Starkrimson/Extensions.git",
    "tag": "0.12.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "4.2"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Extensions/Classes/Core/**/*",
      "resource_bundles": {
        "Localization": [
          "Extensions/Localization/*.lproj/*"
        ]
      }
    },
    {
      "name": "Rx",
      "source_files": "Extensions/Classes/Rx/**/*",
      "dependencies": {
        "Extensions/Core": [

        ],
        "RxSwift": [
          "~> 4.0"
        ],
        "RxCocoa": [
          "~> 4.0"
        ]
      }
    },
    {
      "name": "Snp",
      "source_files": "Extensions/Classes/Snp/**/*",
      "dependencies": {
        "Extensions/Core": [

        ],
        "SnapKit": [
          "~> 4.0"
        ]
      }
    }
  ]
}
